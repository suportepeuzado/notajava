import java.util.Scanner;

public class Main
{
  
	public static void main(String[] args) {
	
	System.out.println("Digite o tamanho do primeiro lado");
	Scanner scan = new Scanner(System.in);
	int n1 = scan.nextInt();
	
	System.out.println("Digite o tamanho do segundo lado");
	
	int n2 = scan.nextInt();
	
	System.out.println("Digite o tamanho do terceiro lado");

	int n3 = scan.nextInt();
	
	if (n1 == n2 && n1 == n3) {System.out.println("Seu triangulo é Equilátero");}
	
	else if(n1 != n2 && n1 != n3 || n2 != n1 && n2 != n3 || n3 != n1 && n3 != n2){System.out.println("Seu triangulo é Escaleno");}
	
    else if(n1 == n2 && n1 != n3 || n1 == n3 && n1 != n2 || n2 == n1 && n2 != n3 || n2 == n3 && n2 != n1 || n3 == n1 && n3 != n2 || n3 == n2 && n3 != n1)
    {System.out.println("Seu triangulo é isósceles");}
    
    else {System.out.println("ERROR");};
	    
	    
	}
    
}
	
	
	
