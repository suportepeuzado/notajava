import java.util.Scanner;

public class Main
{
  
	public static void main(String[] args) {
	
	Scanner scan= new Scanner(System.in);
	
	System.out.println("Digite seu Peso");
	float peso = scan.nextFloat();
	
	System.out.println("Digite sua altura");
	float alt = scan.nextFloat();
	
    float imc = peso/(alt*alt);
	
	if(imc > 40.0) {System.out.println("OBESIDADE GRAVE");}
	
	else if(imc >= 30 && imc <= 39.9) {System.out.println("OBESIDADE");}
	
	else if(imc >= 25.0 && imc <= 29.9) {System.out.println("SOBREPESO");}
	
	else if(imc >= 18.5 && imc <= 24.9) {System.out.println("NORMAL");}
	
	else{System.out.println("MAGREZA");
	}
}
}

