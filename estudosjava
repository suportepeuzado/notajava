/******************************************************************************

Welcome to GDB Online.
GDB online is an online compiler and debugger tool for C, C++, Python, Java, PHP, Ruby, Perl,
C#, OCaml, VB, Swift, Pascal, Fortran, Haskell, Objective-C, Assembly, HTML, CSS, JS, SQLite, Prolog.
Code, Compile, Run and Debug online from anywhere in world.

*******************************************************************************/
import java.util.Scanner;

public class Main
{
  
	public static void main(String[] args) {
	
	System.out.println("De um valor a A");
	Scanner scan = new Scanner(System.in);
	int a = scan.nextInt();
	
	System.out.println("De um valor a B");
    int b = scan.nextInt();
    
    System.out.println("De um valor a C");
    int c = scan.nextInt();
    
   if (a > b && a > c) {
       System.out.println("O maior valor é A = " +a );
    }
    
    else if (b > a && b > c) {
        System.out.println("O maior valor é B = " +b);
    }
        
    else if (c > a && c > b) {
        System.out.println("O maior valor é C = " +c);
    }
    
    else {
     System.out.println("Todos valores são iguais ou existe inconsistência");   
    }
	}
}













